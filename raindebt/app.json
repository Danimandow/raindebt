{
  "expo": {
    "name": "RainDebt",
    "slug": "raindebt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "raindebt",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1B4D3E"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.raindebt.app",
      "config": {
        "googleMapsApiKey": ""
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1B4D3E"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.raindebt.app",
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      },
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION"]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "RainDebt needs your location to add your property coordinates."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
